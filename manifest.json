{
  "default_title": "NEIAAC Webcam",
  "version": "2.0",
  "description": "Check who is on NEIAAC via our WebCam. Try our website http://nei.dei.uc.pt/",

  "browser_action": {
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },
  "default_icon": { "16": "icon16.png",
                    "48": "icon48.png",
                  "128": "icon128.png" },
  "permissions": [
    "http://nei.dei.uc.pt/"
  ],
  "content_scripts": [
      {
          "matches": ["http://jquery.com/*"],
          "js": ["jquery.min.js"]
      }
  ]
}

